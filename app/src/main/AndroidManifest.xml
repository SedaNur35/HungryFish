<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seda.oyun">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconfish"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconfish"
        android:supportsRtl="true"
        android:theme="@style/Theme.Oyun">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-9199482281864772~5097883045"/>
        <activity
            android:name=".SonucEkrani"
            android:exported="false"
            />
        <activity
            android:name=".OyunEkrani"
            android:exported="false"
            />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>